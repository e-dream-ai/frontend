    "serve": "vite preview",
    "lint": "eslint . --ext .js,.ts",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint . --fix --ext .js,.ts",
    "prepare": "if [ -z \"$HUSKY_SKIP_INSTALL\" ]; then husky install; fi"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.20",
