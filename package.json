    "typescript": "^5.0.2",
    "vite": "^5.0.10",
    "vite-tsconfig-paths": "^4.2.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
